<Window x:Class="EstadisticaDeAlumnos3_WPF_Core.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EstadisticaDeAlumnos3_WPF_Core"
        xmlns:controls="clr-namespace:EstadisticaDeAlumnos3_WPF_Core.View"
        mc:Ignorable="d"
        Title="Examenes" Height="450" Width="800" ResizeMode="NoResize">
    <TabControl x:Name="TabControl" SelectionChanged="TabControl_SelectionChanged">
        <TabItem Name ="Students" Header="Students">
            <controls:StudentsView></controls:StudentsView>
        </TabItem>
        <TabItem Name="Subjects" Header="Subjects" >
            <controls:SubjectView></controls:SubjectView>
        </TabItem>
        <TabItem Name="StudentSubject" Header="StudentSubject" >
            <controls:StudentSubjectView></controls:StudentSubjectView>
        </TabItem>
        <TabItem Name="StudentSubjectNew" Header="StudentSubjectNew" >
            <controls:StudentSubjectNewView></controls:StudentSubjectNewView>
        </TabItem>

    </TabControl>
</Window>
